{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithClientIp" ]
  },
  "ConnectionStrings": {
    "DBConnection": "Server=.\\SQLEXPRESS;Database=QualLMS;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "AppSettings": {
    "API": "https://localhost:7118/api/"
  },
  "JwtConfig": {
    "Key": "abcdefghijklmnopqrstuvwxyz1234567890",
    "Issuer": "https://localhost:7118",
    "Audience": "https://localhost:7118"
  },
  "AllowedHosts": "*"
}
